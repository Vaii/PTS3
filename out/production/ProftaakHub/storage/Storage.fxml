<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="storage.StorageController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <Button id="button" fx:id="authorizebutton" defaultButton="false" layoutX="67.0" layoutY="62.0" onAction="#handleButtonActionAuthenticate" text="authorize dropbox" />
        <Label fx:id="label" layoutX="91.0" layoutY="98.0" minHeight="16.0" minWidth="69.0" />
        <TextField id="inputtb" fx:id="inputtxt" layoutX="32.0" layoutY="106.0" prefWidth="200.0" />
        <Button fx:id="sendbutton" layoutX="73.0" layoutY="158.0" mnemonicParsing="false" onAction="#handleButtonActionSendAuthenticate" text="send auth token" />
        <TextArea fx:id="outputtxt" layoutX="26.0" layoutY="186.0" prefHeight="39.0" prefWidth="200.0" wrapText="true" />
        <ListView fx:id="fileList" layoutX="271.0" layoutY="39.0" onMouseClicked="#folderDoubleClick" prefHeight="200.0" prefWidth="277.0" />
        <Button fx:id="btnDownload" layoutX="271.0" layoutY="261.0" mnemonicParsing="false" onAction="#downloadFile" text="Download" />
        <Button fx:id="btnUpload" layoutX="492.0" layoutY="261.0" mnemonicParsing="false" onAction="#uploadFile" text="Upload" />
        <Button fx:id="btnBack" layoutX="271.0" layoutY="14.0" mnemonicParsing="false" onAction="#backDirectory" text="Back" />
    </children>
</AnchorPane>
